{
	"manifest_version": 2,
	"name": "Emoji to English",
	"version": "2.1",
	"description": "Translates emojis to their Unicode names",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": [
				"names-dict.js", 
				"emoji-replacer.js", 
				"dom-manipulator.js",
				"orchestrator.js" 
			],
			"css": ["emoji-to-english.css"]
		}
	],
	"applications": {
		"gecko": {
			"id": "emojisToEnglish@szupie"
		}
	},
	"options_ui": {
		"page": "settings-panel/settings.html"
	},
	"permissions": ["storage"],
	"default_locale": "en"
}
